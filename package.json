  "license": "none",
  "bugs": {
    "url": "https://github.com/ethoinformatics/sample-ui-browserify/issues"
  },
  "homepage": "https://github.com/ethoinformatics/sample-ui-browserify",
  "dependencies": {
    "brfs": "^1.2.0",
    "browserify": "^5.9.1",
    "lodash": "^2.4.1",
    "mkdirp": "^0.5.0",
    "node-lessify": "0.0.4",
    "reactive": "^1.2.0",
    "vashify": "^0.3.3"
  },
  "devDependencies": {
    "nodemon": "^1.2.1"
  }
}
